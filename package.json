{
  "name": "@imcyk/context-menu",
  "version": "0.1.14",
  "private": false,
  "author": "chen yankai",
  "license": "MIT",
  "description": "基于vue3+vite+ts的右键菜单组件，适用于组合式api和选项式api。",
  "keywords": [
    "vue3",
    "vue",
    "context menu",
    "menu",
    "右键菜单",
    "contextmenu"
  ],
  "homepage": "https://github.com/cykcykcykcyk/vue3-context-menu",
  "bugs": "https://github.com/cykcykcykcyk/vue3-context-menu/issues",
  "main": "./dist/context-menu.umd.js",
  "module": "./dist/context-menu.es.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/context-menu.es.js",
      "require": "./dist/context-menu.umd.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./style/config.scss": {
      "import": "./style/config.scss",
      "require": "./style/config.scss"
    },
    "./style/index.scss": {
      "import": "./style/index.scss",
      "require": "./style/index.scss"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "genlog": "conventional-changelog - p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "magic-string": "^0.26.7",
    "rollup-plugin-copy": "^3.4.0",
    "sass": "^1.56.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vite-plugin-dts": "^1.7.0",
    "vue-tsc": "^1.0.9"
  },
  "files": [
    "package.json",
    "dist",
    "style"
  ],
  "types": "./dist/index.d.ts"
}
